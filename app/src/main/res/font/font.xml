<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

<!-- Weight is the overall thickness of
 a typeface's stroke in any given font.-->

    <font
        android:font="@font/roboto_regular"
        app:fontStyle="normal"
        tools:targetApi="o" />

    <font
        android:font="@font/roboto_medium"
        app:fontStyle="normal"
        tools:targetApi="o" />


    <font
        android:font="@font/nunito_semibold"
        app:fontStyle="normal"
        tools:targetApi="o" />



</font-family>